From principles to prima facie reasons via epistemic default reasoning
[
 {
    "id": 9,
    "premise": "You shall do no harm.",
    "hypothesis": "You shall not trash talk Bob.",
    "label": "Specification and default reasoning",
    "explanation": "Trash talking typically causes distress. Given that you shall do no harm, the fact that trash talking causes distress is a reason not to trash talk Bob."
  },
{
    "id": 10,
    "premise": "You should help others. Bob is crying.",
    "hypothesis": "You should comfort Bob.",
    "label": "Specification and default reasoning",
    "explanation": "Crying typically indicates distress. Given that you should help others, the fact that Bob is in distress is a reason to comfort him."
  },
{
    "id": 11,
    "premise": "You should respect others’ autonomy. Bob says that he is a vegetarian.",
    "hypothesis": "You should not offer Bob any meat.",
    "label": "Specification and default reasoning",
    "explanation": "People who say they are vegetarians typically do not want meat. Given that you should respect others’ autonomy, the fact that Bob does not want meat is a reason not to offer him any."
  },
  {
    "id": 12,
    "premise": "You should respect others' autonomy. Bob shys away from your attention.",
    "hypothesis": "You should leave Bob in peace.",
    "label": "Specification and default reasoning",
    "explanation": "Shying away typically indicates a desire for distance. Given that you should respect others’ autonomy, the fact that Bob wants distance is a reason to leave him in peace."
  }
]
